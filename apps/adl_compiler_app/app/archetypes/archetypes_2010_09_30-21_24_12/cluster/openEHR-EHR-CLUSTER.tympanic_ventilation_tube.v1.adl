	>
	details = <
		["en"] = <
			language = <[ISO_639-1::en]>
			purpose = <"To record details of a Tympanic Ventilation tube.">
			use = <"To record details of a Tympanic Ventilation tube as part of an inspection of the tympanic membrane.">
